					<attribute name="attrs">{'invisible':[('state_ids','!=',[])]}
					</attribute>
				</xpath>
			</field>
		</record>




		<record model="ir.ui.view"
			id="helpdesk_ticket_view_form">
			<field name="name">helpdesk.ticket</field>
			<field name="model">helpdesk.ticket</field>
			<field name="inherit_id"
